<%= render partial: "layouts/errors", locals: {object: @contact} %>

<h2>Add a New Contact</h2>

<%= form_for (@contact) do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name%>
    <br>

    <%= f.label :description %>
    <%= f.text_field :description%>
    <br>

    <%= f.label :notes %>
    <%= f.text_area :notes%>
    <br>

    <%= f.submit "Add New Contact"%>

<%end%>

